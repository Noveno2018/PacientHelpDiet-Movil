<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="diet.help.pacient.pacienthelpdiet">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_heart"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_heart"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Vistas.Login_Activity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/app_name" />
        <activity
            android:name=".Vistas.SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Vistas.PanelNavegacion"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Pruebas" />
        <activity android:name=".Vistas.Dieta_Activity"></activity>
        <activity android:name=".Vistas.Consulta_Dietas_Activity"></activity>
    </application>

</manifest>